node
{
  
    stage('create')
    {
        echo "Create a cluster"
        aws cloudformation describe-stacks --stack-name miniocluster111 --region us-east-1      
    }
    stage('fetch')
    {
        echo "Fetch cluster address"
    }
    stage('build')
    {
        echo "Build a container"
    }
    stage('test')
    {
        echo "Run the test script"
    }
    stage('Delete')
    {
        echo "Delete the cluster"
    }
}
